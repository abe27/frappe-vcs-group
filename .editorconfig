# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.{js,jsx}]
indent_style = space
indent_size = 2

[*.{json}]
indent_style = space
indent_size = 3


[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false